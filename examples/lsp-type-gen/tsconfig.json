{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "./dist",
    "plugins": [
      {
        "name": "data-plugin/dist/index.js",
        "filePattern": "**/example/schema.ts"
      }
    ]
  }
}
